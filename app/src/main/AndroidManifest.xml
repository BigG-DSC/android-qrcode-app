<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eitdigital.big.brigtheyedapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".access.Welcome_Activity"
            android:theme="@style/AppTheme.NoActionBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".access.Access_Activity"
            android:theme="@style/AppTheme.NoActionBar.Fullscreen" />
        <activity
            android:name=".doctor.DashboardDoctor_Activity"
            android:label="@string/title_activity_dashboard_doctor_" />
        <activity
            android:name=".patient.DashboardPatient_Activity"
            android:label="@string/title_activity_dashboard_patient_"></activity>
    </application>

</manifest>